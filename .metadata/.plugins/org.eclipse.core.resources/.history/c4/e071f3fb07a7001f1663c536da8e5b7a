package app67CollectionApi;
import java.util.ArrayList;

public class M1 {
	public static void main(String[] args) {
		ArrayList list = new ArrayList();
		list.add(false);
		list.add('a');
		list.add(12.5);
		
		list.add("abc");
		list.add(123);
		list.remove(2);
		list.remove(0);
		list.remove(3);
		System.out.println(list);
		//System.out.println(list.get(2));
	}
}
