package app64;
class C implements Runnable {
	public void run() {
		for(int i = 3000; i < 4000; i++) {
			System.out.println(i);
		}
	}
} 
class D implements Runnable {
	
		public void run()
		{
			for(int i = 3000; i < 4000; i++) {
				System.out.println(i);
		}
	}
}
	
public class M3{
	public static void main(String[] args) {
		C c1 = new C();
		Thread t1 = new Thread();
		t1.start();
		
		D d1 = new D();
		Thread t2 = new Thread();
		t2.start();
		
		
		for(int i = 13000; i < 14000; i++) {
			System.out.println(i);
		}
	}
}
