package Genric;

class AB < T >
{
        T obj;
}
public class M61
{
	
	
	public static void main(String[] args) 
    {
            AB<Integer> a1 = new AB<>();
            AB a2 = new AB();
            a1 = a2;
            a2 = a1;
            a1.obj = 1000;
            a2.obj = 1000;
            int i = a1.obj;
            //int j = a2.obj;
            int j = (Integer) a2.obj;
            System.out.println("done");
    }
	
