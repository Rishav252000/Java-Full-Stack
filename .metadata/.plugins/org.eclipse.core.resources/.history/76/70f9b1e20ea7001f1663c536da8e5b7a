package removeIf;

import java.util.ArrayList;

public class M2 {
	public static void main(String[] args) {
		ArrayList<Integer> arr = new ArrayList<Integer>();
		arr.add(123);
		arr.add(12);
		arr.add(12111);
		arr.add(124);
		arr.add(54);
		arr.add(12);
		arr.add(123);
		System.out.println(arr.get(0));
		System.out.println(arr);
		System.out.println(arr.contains(121));
		
	}
}
